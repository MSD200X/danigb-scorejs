{
  "name": "scorejs",
  "version": "0.10.0",
  "description": "Create and manipulate musical scores with javascript",
  "main": "index.js",
  "scripts": {
    "test": "mocha test/",
    "dist": "browserify index.js > dist/score.js && uglifyjs dist/score.js > dist/score.min.js",
    "docs": "browserify index.js | documentation -f md > API.md",
    "jsdoc": "jsdoc -c .jsdoc3.json && open docs/index.html",
    "disc": "mkdir -p tmp && browserify --full-paths index.js | discify > tmp/disc.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danigb/scorejs.git"
  },
  "keywords": [
    "music",
    "theory",
    "chords",
    "melody",
    "score",
    "leadsheet",
    "jazz"
  ],
  "author": "danigb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danigb/scorejs/issues"
  },
  "homepage": "https://github.com/danigb/scorejs#readme",
  "devDependencies": {
    "eslint": "^2.5.1",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "mocha": "^2.4.5"
  },
  "dependencies": {
    "chord-dictionary": "^0.2.0",
    "midi-freq": "^1.0.0",
    "note-midi": "^1.0.2",
    "note-transposer": "^1.0.4"
  },
  "standard": {
    "ignore": [
      "dist/**"
    ]
  }
}
